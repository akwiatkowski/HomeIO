<h1>Listing action_events</h1>

<table>
  <tr>
    <th>Time</th>
    <th>Error status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @action_events.each do |action_event| %>
  <tr>
    <td><%= action_event.time %></td>
    <td><%= action_event.error_status %></td>
    <td><%= link_to 'Show', action_event %></td>
    <td><%= link_to 'Edit', edit_action_event_path(action_event) %></td>
    <td><%= link_to 'Destroy', action_event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

